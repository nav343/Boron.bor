{
  "name": "projectboron",
  "version": "0.0.1",
  "description": "A multi-purpose programming language written in Typescript",
  "main": "./dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "build": "pkg --compress GZip ./dist/index.js --o ./dist/boronc -t latest-linux-x64"
  },
  "author": "nav343",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^18.11.18",
    "@types/prompt-sync": "^4.2.0",
    "prompt-sync": "^4.2.0"
  },
  "bin": {
    "boronc": "./dist/index.js"
  },
  "pkg": {
    "scripts": "./dist/**/*.js",
    "targets": [
      "latest-linux-arm64"
    ],
    "outputPath": "dist",
    "compress": "GZip"
  }
}
